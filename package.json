{
  "name": "todoeast",
  "version": "1.0.0",
  "description": "Todo App - Built to learn Testing in JavaScript and React",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,css,json}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "dev": "parcel src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NathnaelDS/todoeast.git"
  },
  "keywords": [
    "todo"
  ],
