  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NathnaelDS/todoeast/issues"
  },
  "homepage": "https://github.com/NathnaelDS/todoeast#readme",
  "dependencies": {
    "postcss-modules": "^1.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tailwindcss": "^1.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2"
  }
